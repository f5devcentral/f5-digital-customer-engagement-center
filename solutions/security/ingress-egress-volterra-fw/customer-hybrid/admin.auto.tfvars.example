#    workstation     = true
#  }
#}

# Azure variables
azureLocation        = "westus2"
volterraCloudCredAzure = "cloud-cred-azure"

# GCP variables
gcpRegion = "us-west1"
gcpProjectId = "my-gcp-project-id"
volterraCloudCredGCP = "cloud-cred-gcp"
